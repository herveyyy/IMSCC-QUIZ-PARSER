{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/upload",
            "dest": "server.js",
            "methods": ["POST"]
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ]
}
